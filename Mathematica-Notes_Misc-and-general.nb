Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Evaluate", "[", 
       RowBox[{"ToExpression", "[", 
        RowBox[{"\"\<variable$\>\"", "<>", 
         RowBox[{"ToString", "[", "i", "]"}]}], "]"}], "]"}], "=", "i"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "10"}], "}"}]}], "]"}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "but", " ", "this", " ", "can", " ", "be", " ", "equivalently", " ", 
    "done", " ", 
    RowBox[{"by", ":"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
