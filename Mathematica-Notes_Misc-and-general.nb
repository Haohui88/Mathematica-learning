  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"this", " ", "gives", " ", "a", " ", "list"}], ",", " ", 
    RowBox[{"so", " ", "var", " ", "1", " ", "is", " ", "a", " ", 
     RowBox[{"list", ".", " ", "To"}], " ", "extract", " ", "the", " ", 
     "value"}], ",", " ", 
    RowBox[{"use", ":"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"var1", "=", 
   RowBox[{"Extract", "[", 
    RowBox[{"list1", ",", 
     RowBox[{"First", "@", 
      RowBox[{"Position", "[", 
       RowBox[{"list2", ",", 
        RowBox[{"n_", "/;", 
         RowBox[{"n", "\[Equal]", 
          RowBox[{"Max", "[", "list2", "]"}]}]}]}], "]"}]}]}], "]"}]}], 
