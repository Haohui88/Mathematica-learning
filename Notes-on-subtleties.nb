  RowBox[{
   RowBox[{"fn3", "[", "a_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"fn", ",", "quotient", ",", "remain", ",", "x"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"fn", "=", 
       RowBox[{"Piecewise", "[", 
        RowBox[{
         RowBox[{"{", 
