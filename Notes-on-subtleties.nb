354bdf84894a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"fn1", "=", 
   RowBox[{"Piecewise", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"4", "*", "x"}], ",", 
         RowBox[{"0", "\[LessEqual]", "x", "<", "0.5"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         FractionBox["1", "3"], ",", 
         RowBox[{"0.5", "\[LessEqual]", "x"}]}], "}"}]}], "}"}], ",", 
     "Null"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fn2", "[", "a_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"quotient", ",", "remain"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"quotient", ",", "remain"}], "}"}], "=", 
       RowBox[{"QuotientRemainder", "[", 
        RowBox[{"a", ",", "0.2"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"fn1", "/.", 
       RowBox[{"x", "\[Rule]", "remain"}]}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
